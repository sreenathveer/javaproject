<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>veeraiahgari</display-name>
  <servlet>
  	<servlet-name>home</servlet-name>
  	<servlet-class>com.sree.servlets.HomeServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>cs</servlet-name>
  	<servlet-class>com.sree.servlets.ConnectionServlet</servlet-class>
  	
  	<init-param>
  		<param-name>driver</param-name>
  		<param-value>com.mysql.jdbc.Driver</param-value>
  	</init-param>
  	<init-param>
  		<param-name>host</param-name>
  		<param-value>jdbc:mysql://localhost:3306/</param-value>
  	</init-param>
  	<init-param>
  		<param-name>user</param-name>
  		<param-value>root</param-value>
  	</init-param>
  	<init-param>
  		<param-name>pass</param-name>
  		<param-value></param-value>
  	</init-param>
  	<init-param>
  		<param-name>dbName</param-name>
  		<param-value>veeraiahgari</param-value>
  	</init-param>
  </servlet>
  <servlet-mapping>
  	<servlet-name>cs</servlet-name>
  	<url-pattern>/cs</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>home</servlet-name>
  	<url-pattern>/home</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>